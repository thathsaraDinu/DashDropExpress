{
  "name": "my-monorepo",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "frontend",
    "backend"
  ],
  "scripts": {
    "install-all": "npm install --prefix frontend && npm install --prefix backend",
    "start": "start npm run start-frontend & start npm run start-backend",
    "start-frontend": "npm start --prefix frontend",
    "start-backend": "npm start --prefix backend",
    "test": "npm-run-all --parallel test-frontend test-backend",
    "test-frontend": "npm test --prefix frontend",
    "test-backend": "npm test --prefix backend"
  },
  "dependencies": {
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.15.6",
    "@mui/material": "^5.15.14",
    "@testing-library/jest-dom": "^5.17.0",
    "axios": "^1.6.5",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "jspdf": "^2.5.1",
    "jspdf-autotable": "^3.8.2",
    "jwt-decode": "^4.0.0",
    "mongoose": "^8.1.1",
    "my-monorepo": "file:..",
    "nodemailer": "^6.9.13",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^5.1.0",
    "react-router-dom": "^6.21.3",
    "react-scripts": "5.0.1",
    "serve": "^14.2.1",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "tailwindcss": "^3.4.1"
  }
}
