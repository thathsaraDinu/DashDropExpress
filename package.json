{
  "name": "my-monorepo",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "frontEnd",
    "backEnd"
  ],
  "scripts": {
    "install-all": "npm install --prefix frontEnd && npm install --prefix backEnd",
    "start": "start npm run start-frontEnd & start npm run start-backEnd",
    "start-frontEnd": "npm start --prefix frontEnd",
    "start-backEnd": "npm start --prefix backEnd",
    "test": "npm-run-all --parallel test-frontEnd test-backEnd",
    "test-frontEnd": "npm test --prefix frontEnd",
    "test-backend": "npm test --prefix backEnd"
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.1.1",
    "nodemailer": "^6.9.13",
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.15.6",
    "@mui/material": "^5.15.14",
    "@testing-library/jest-dom": "^5.17.0",
    "axios": "^1.6.5",
    "jwt-decode": "^4.0.0",
    "my-monorepo": "file:..",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.21.3",
    "react-scripts": "5.0.1",
    "serve": "^14.2.1",
    "web-vitals": "^2.1.4",
    "react-icons": "^5.1.0"
  },
  "devDependencies": {
    "tailwindcss": "^3.4.1"
  }
}